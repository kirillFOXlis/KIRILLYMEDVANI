<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#444141">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="107dp"
        android:layout_height="82dp"
        android:background="#444141"
        android:tint="#D1CECE"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="649dp"
        tools:ignore="ContentDescription,MissingConstraints,UseAppTint" />

    <Button
        android:id="@+id/button5"
        android:layout_width="113dp"
        android:layout_height="60dp"
        android:backgroundTint="#2CCA2C"
        android:fontFamily="serif"
        android:text="Поиск"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="156dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="61dp"
        android:layout_height="65dp"
        android:importantForAccessibility="no"
        app:srcCompat="@android:drawable/ic_menu_search"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="156dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="66dp"
        android:layout_height="72dp"
        app:srcCompat="@android:drawable/btn_star_big_on"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="6dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="370dp"
        android:layout_height="65dp"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:fontFamily="serif"
        android:importantForAutofill="no"
        android:inputType="text"
        android:text="Введите название или ингридиенты"
        android:textColor="#F6F4F3"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="64dp" />

</androidx.constraintlayout.widget.ConstraintLayout>