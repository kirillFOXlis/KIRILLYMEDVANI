<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#444141">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="107dp"
        android:layout_height="82dp"
        android:background="#444141"
        android:tint="#D1CECE"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="633dp"
        tools:ignore="ContentDescription,MissingConstraints,UseAppTint" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="257dp"
        android:layout_height="50dp"
        android:backgroundTint="#F4EFEF"
        android:ems="10"
        android:fontFamily="monospace"
        android:importantForAutofill="no"
        android:inputType="text"
        android:text="Найдено 3 рецепта"
        android:textColor="#A09C9C"
        android:textSize="26sp"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="235dp"
        android:layout_height="23dp"
        android:fontFamily="serif"
        android:text="Томатный суп - пюре"
        android:textAlignment="center"
        android:textColor="#A09C9C"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="78dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="Помидоры по - корейски"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#A09C9C"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="270dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:importantForAccessibility="no"
        android:text="Писто ( овощное рагу )"
        android:textAlignment="center"
        android:textColor="#A09C9C"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="407dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton7"
        style="@android:style/Widget.DeviceDefault.Light.ImageButton"
        android:layout_width="162dp"
        android:layout_height="125dp"
        android:importantForAccessibility="no"
        android:scaleType="fitEnd"
        app:srcCompat="@drawable/__max"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="109dp"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="167dp"
        android:layout_height="129dp"
        android:scaleType="fitEnd"
        app:srcCompat="@drawable/pomidory_po_koreyski"
        tools:ignore="ContentDescription,MissingConstraints,VisualLintBounds,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="286dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="161dp"
        android:layout_height="159dp"
        app:srcCompat="@drawable/big_726828"
        tools:ignore="ContentDescription,ImageContrastCheck,MissingConstraints"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="448dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="222dp"
        android:layout_height="187dp"
        android:fontFamily="serif"
        android:text=" Продукты (на 4 порции) Кабачок – 250 г Баклажан – 250 г Помидоры – 400 г Перец болгарский – 300 г Лук репчатый – 200 г Чеснок – 4 зубчика Масло оливковое – 60 мл (4 ст. ложки) Паприка – 1 ч. ложка Соль – по вкусу Сахар – 0,5 ч. ложки (или по вкусу) Перец молотый (смесь) – по вкусу"
        android:textAlignment="center"
        android:textColor="#FDFDFD"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="439dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="183dp"
        android:layout_height="101dp"
        android:fontFamily="serif"
        android:text="2 болгарских перца; 1 острый перец; 2 головки чеснока; 1 пучок петрушки; 100 мл винного уксуса 6%; 100 мл растительного масла; 80 г сахара; 2 столовые ложки соли; 2 кг помидоров."
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="294dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="229dp"
        android:layout_height="155dp"
        android:fontFamily="serif"
        android:text="Помидоры - 630 г Картофель - 200 г Томатная паста - 70 г Сливки (10-15%) - 40 г Лук репчатый - 80 г Чеснок - 1 зубчик Масло растительное - 1 ст. ложка Соль - 1/2 ч. ложки Перец черный молотый - на кончике ножа Петрушка - 5 г Вода - 600 мл"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textStyle="bold|italic"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="106dp" />

</androidx.constraintlayout.widget.ConstraintLayout>